{
  "name": "todo",
  "version": "1.0.0",
  "description": "building a test to-do app",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./",
    "test": "jest -i --verbose --coverage --runInBand",
    "start": "node index.js",
    "dboff": "killall mongod",
    "watch": "nodemon index.js",
    "dbon": "mkdir -p ./db && mongod --dbpath ./db --replSet rs0",
    "dbprod": "mkdir -p ./dbprod && mongod --dbpath ./dbprod --replSet rs2",
    "dev": "concurrently \"yarn run watch\" \"cd ../frontend && yarn start\""
  },
  "keywords": [
    "todo"
  ],
  "author": "Yitz Deng",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "create-react-app": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "mongoose": "^5.10.11",
    "run-rs": "^0.7.3",
    "superagent": "^6.1.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "eslint": "^7.12.1",
    "nodemon": "^2.0.6"
  }
}
